
pub struct SimulationResult {
    result_table : HashMap<u32, u32>,
    simulation_count : u32,
}

impl SimulationResult {
    pub fn init() -> SimulationResult {
        SimulationResult {
            result_table = HashMap::new();
            simulation_count = 0;
        }
    }

    pub fn addResult(&self, id : u32, count:u32 = 1){
        let score_of_id = all_sims.get(&id);
        let new_score = match score_of_id{
            None => 1,
            Some(sc) => sc + count
        };
        
        self.result_table.insert(id, new_score);
    }
}